    "@radix-ui/react-slot": "^1.1.2",
    "astro": "^5.17.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.6.2",
    "lucide-react": "^0.556.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^4.1.2",
    "tailwind-merge": "^3.0.2"
