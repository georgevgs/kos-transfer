    "build:vite": "tsc -b && vite build",
    "preview:vite": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md,astro}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md,astro}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.7.0",
